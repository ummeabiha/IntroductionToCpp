#include <iostream>
using namespace std;

// FUNCTION OVERLOADING:

int add (int a,int b)
{
    cout<<"Using function with two arguments"<<endl;
    return a+b;
}

int add (int a,int b,int c)
{
    cout<<"Using function with three arguments"<<endl;
    return a+b+c;
}

// calculating volume of cylinder
int volume(double r,int h)
{
    return (3.14*r*r*h);
}

// calculating volume of cube
int volume(int a)
{
    return (a*a*a);
}

// calculating volume of rectangular box
int volume(int l,int b,int h)
{
    return (l*b*h);
}

int main()
{
    cout<<"The sum of 3 and 6 is "<<add(3,6)<<endl;
    cout<<"The sum of 3 and 6 is "<<add(4,7,8)<<endl;
    cout<<"The volume of cylinder is "<<volume(5,10)<<endl;
    cout<<"The volume of cube is "<<volume(3)<<endl;
    cout<<"The volume of rectangular box is "<<volume(3,5,27)<<endl;


    return 0;
}
