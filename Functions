#include <iostream>
using namespace std;

int add(int num1, int num2)
{
    int value;
    value = num1 + num2;
    return value;
}

int main()
{
    int num1, num2;
    cout << "Enter first number" << endl;
    cin >> num1;
    cout << "Enter second number" << endl;
    cin >> num2;
    cout << "the function returned is: " << add(num1, num2)
  
    return 0;
}
